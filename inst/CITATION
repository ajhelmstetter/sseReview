citHeader("To cite sseReview in publications use:")

bibentry(
  bibtype = "Manual",
  title   = "sseReview: {An} {R} package to ...",
  author  = c(person("Helmstetter Andrew")),
  year    = "2021",
  note    = "R package version 0.0.0.9000",
  url     = "https://github.com/ajhelmstetter/sseReview"
)
